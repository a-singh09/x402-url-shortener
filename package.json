{
  "name": "url-shortener-x402",
  "version": "1.0.0",
  "description": "URL shortener service with x402 payment integration on Base Sepolia",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "clean": "rm -rf dist",
    "db:setup": "echo 'Run: createdb url_shortener && psql url_shortener < schema.sql'",
    "db:reset": "echo 'Run: dropdb url_shortener && createdb url_shortener && psql url_shortener < schema.sql'"
  },
  "keywords": [
    "url-shortener",
    "x402",
    "blockchain",
    "payments",
    "base-sepolia"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.16.3",
    "x402-express": "^0.7.0"
  }
}
