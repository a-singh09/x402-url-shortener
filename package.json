{
  "name": "url-shortener-x402",
  "version": "1.0.0",
  "description": "URL shortener service with x402 payment integration on Base Sepolia",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/startup.js",
    "start:app": "node dist/index.js",
    "dev": "ts-node src/startup.ts",
    "dev:app": "ts-node src/index.ts",
    "test": "jest",
    "clean": "rm -rf dist",
    "db:setup": "echo 'Run: createdb url_shortener && psql url_shortener < schema.sql'",
    "db:reset": "echo 'Run: dropdb url_shortener && createdb url_shortener && psql url_shortener < schema.sql'",
    "health": "curl -f http://localhost:3000/health || exit 1"
  },
  "keywords": [
    "url-shortener",
    "x402",
    "blockchain",
    "payments",
    "base-sepolia"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "pg": "^8.16.3",
    "viem": "^2.38.6",
    "x402-axios": "^0.7.0",
    "x402-express": "^0.7.0",
    "x402-fetch": "^0.7.0"
  }
}
